@model ShopPizzaHut.Models.EF.LoaiKhachHang

@{
    ViewBag.Title = "Chi tiết";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Chi tiết</h2>

<div>
    <h4>Loại khách hàng</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Tên loại khách hàng
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TenLoaiKH)
        </dd>

    </dl>
</div>
<p>
    <a href="@Url.Action( "Edit", new { id = Model.MaLoaiKH })" class="btn btn-default" style="background:#4e73df; color:white;">Sửa</a>
</p>
<p>
    @Html.ActionLink("Trở lại trang trước", "Index")
</p>
