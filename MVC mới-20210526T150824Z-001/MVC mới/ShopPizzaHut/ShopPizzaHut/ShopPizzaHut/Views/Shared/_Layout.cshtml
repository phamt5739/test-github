<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Josefin+Sans" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Nothing+You+Could+Do" rel="stylesheet">

    <link rel="stylesheet" href="/Content/client/css/open-iconic-bootstrap.min.css">
    <link rel="stylesheet" href="/Content/client/css/animate.css">

    <link rel="stylesheet" href="/Content/client/css/owl.carousel.min.css">
    <link rel="stylesheet" href="/Content/client/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="/Content/client/css/magnific-popup.css">

    <link rel="stylesheet" href="/Content/client/css/aos.css">

    <link rel="stylesheet" href="/Content/client/css/ionicons.min.css">

    <link rel="stylesheet" href="/Content/client/css/bootstrap-datepicker.css">
    <link rel="stylesheet" href="/Content/client/css/jquery.timepicker.css">


    <link rel="stylesheet" href="/Content/client/css/flaticon.css">
    <link rel="stylesheet" href="/Content/client/css/icomoon.css">
    <link rel="stylesheet" href="/Content/client/css/style.css">
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
        <div class="container">
            <a class="navbar-brand" href="/Home"><img src="/Content/client/images/logo.jpg" height="80" /></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="oi oi-menu"></span> Menu
            </button>
            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item"><a href="/Home" class="nav-link">Trang chủ</a></li>
                    <li class="nav-item"><a href="/SanPhams" class="nav-link">Sản phẩm</a></li>
                    <li class="dropdown nav-item">
                        <a class="dropdown-toggle nav-link" data-toggle="dropdown" href="#">
                            Loại sản phẩm
                            <span class="caret"></span>
                        </a>
                        @{Html.RenderAction("Index", "Menu"); }
                    </li>
                    <li class="nav-item"><a href="/AboutUs" class="nav-link">Giới thiệu</a></li>
                    <li class="nav-item"><a href="/Giohang" class="nav-link">Giỏ hàng</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- END nav -->

    @RenderBody()

    <footer class="ftco-footer ftco-section img">
        <div class="overlay"></div>
        <div class="container">
            <div class="row mb-5">
                <div class="col-lg-3 col-md-6 mb-5 mb-md-5">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2">Cần sự hỗ trợ</h2>
                        <ul class="ftco-footer-social list-unstyled float-md-left float-lft mt-5">
                            <li><a style="cursor:pointer;"><span class="icon-twitter"></span></a></li>
                            <li><a style="cursor:pointer;"><span class="icon-facebook"></span></a></li>
                            <li><a style="cursor:pointer;"><span class="icon-instagram"></span></a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-2 col-md-6 mb-5 mb-md-5">
                    <div class="ftco-footer-widget mb-4 ml-md-4">
                        <h2 class="ftco-heading-2">Về chúng tôi</h2>
                        <ul class="list-unstyled">
                            <li><a href="/AboutUs" class="py-2 d-block">Giới thiệu</a></li>
                            <li><a href="/AboutUs" class="py-2 d-block">Tầm nhìn</a></li>
                            <li><a href="/AboutUs" class="py-2 d-block">Giá trị cốt lõi</a></li>
                            <li><a href="/AboutUs" class="py-2 d-block">Văn hóa</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6 mb-5 mb-md-5">
                    <div class="ftco-footer-widget mb-4 ml-md-4">
                        <h2 class="ftco-heading-2">Thông tin tuyển dụng</h2>
                        <ul class="list-unstyled">
                            <li><a style="cursor:pointer;" class="py-2 d-block">Làm việc tại Pizza Hut</a></li>
                            <li><a style="cursor:pointer;" class="py-2 d-block">Môi trường làm việc</a></li>
                            <li><a style="cursor:pointer;" class="py-2 d-block">Cơ hội phát triển nghề nghiệp</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6 mb-5 mb-md-5">
                    <div class="ftco-footer-widget mb-4">
                        <h2 class="ftco-heading-2">Liên hệ với Pizza Hut</h2>
                        <div class="block-23 mb-3">
                            <ul>
                                <li><span class="icon icon-map-marker"></span><span class="text">28 Xuân La - Tây Hồ - Hà Nội</span></li>
                                <li><a style="cursor:pointer;"><span class="icon icon-phone"></span><span class="text">1900 1822</span></a></li>
                                <li><a style="cursor:pointer;"><span class="icon icon-address-card"></span><span class="text">info@yourdomain.com</span></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>



    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>


    <script src="/Content/client/js/jquery.min.js"></script>
    <script src="/Content/client/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="/Content/client/js/popper.min.js"></script>
    <script src="/Content/client/js/bootstrap.min.js"></script>
    <script src="/Content/client/js/jquery.easing.1.3.js"></script>
    <script src="/Content/client/js/jquery.waypoints.min.js"></script>
    <script src="/Content/client/js/jquery.stellar.min.js"></script>
    <script src="/Content/client/js/owl.carousel.min.js"></script>
    <script src="/Content/client/js/jquery.magnific-popup.min.js"></script>
    <script src="/Content/client/js/aos.js"></script>
    <script src="/Content/client/js/jquery.animateNumber.min.js"></script>
    <script src="/Content/client/js/bootstrap-datepicker.js"></script>
    <script src="/Content/client/js/jquery.timepicker.min.js"></script>
    <script src="/Content/client/js/scrollax.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
    <script src="/Content/client/js/google-map.js"></script>
    <script src="/Content/client/js/main.js"></script>

</body>
</html>